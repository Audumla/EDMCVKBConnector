        {
          "flags": {
            "all_of": ["FlagsHardpointsDeployed"]
          }
        }
      ]
    },
    "then": {
      "vkb_set_shift": ["Shift1"]
    },
    "else": {
      "vkb_clear_shift": ["Shift1"]
    }
  },
  {
    "id": "dashboard_galaxy_map",
